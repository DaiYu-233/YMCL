<Page x:Class="YMCL.LoginUI.Microsoft"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:YMCL.LoginUI"
      mc:Ignorable="d" Title="Microsoft"
      d:DesignHeight="322" d:DesignWidth="680">

    <Grid HorizontalAlignment="Center" VerticalAlignment="Top" Height="322" Width="680">
        <TextBox x:Name="MicrosoftPlayerName" HorizontalAlignment="Center" Margin="0,113,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Width="180" IsReadOnly="True" Height="24" FontSize="14" FontFamily="Noto Sans CJK SC Medium" TextAlignment="Center"/>
        <TextBox x:Name="MicrosoftPlayerUUID" HorizontalAlignment="Center" TextWrapping="Wrap" Text="UUID  请登录您的Microsoft账户" VerticalAlignment="Top" Width="330" IsReadOnly="True" Height="24" FontSize="14" FontFamily="Noto Sans CJK SC Medium" TextAlignment="Center" Margin="0,161,0,0"/>

    </Grid>
</Page>
